<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="paneMenu" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="15.0"
      stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controller.library.MenuLibraryController">
   <Label alignment="CENTER" contentDisplay="TOP" prefHeight="57.0" prefWidth="245.0" text="ŚWIAT FILMÓW"
          textFill="#d9bc4a" wrapText="true">
      <font>
         <Font name="System Bold" size="23.0"/>
      </font>
      <VBox.margin>
         <Insets left="7.0"/>
      </VBox.margin>
   </Label>
   <Label alignment="CENTER" contentDisplay="TOP" prefHeight="49.0" prefWidth="250.0" text="MOVIEWORLD"
          textFill="#d9bc4a" wrapText="true">
      <font>
         <Font name="System Bold" size="33.0"/>
      </font>
      <VBox.margin>
         <Insets bottom="15.0" top="-25.0"/>
      </VBox.margin>
   </Label>
   <Button id="showMovie" fx:id="id_showMovie" defaultButton="true" mnemonicParsing="false" onAction="#showMovie"
           opacity="0.4" prefHeight="70.0" prefWidth="245.0" text="LISTA FILMÓW">
      <VBox.margin>
         <Insets left="7.0"/>
      </VBox.margin>
   </Button>
   <Button id="addMovie" fx:id="id_addMovie" defaultButton="true" mnemonicParsing="false" onAction="#addMovie"
           opacity="0.4" prefHeight="70.0" prefWidth="245.0" text="DODAWANIE FILMU">
      <VBox.margin>
         <Insets left="7.0"/>
      </VBox.margin>
   </Button>
   <Button id="deleteMovie" fx:id="id_deleteMovie" defaultButton="true" mnemonicParsing="false" onAction="#deleteMovie"
           opacity="0.4" prefHeight="70.0" prefWidth="245.0" text="USUWANIE FILMU">
      <VBox.margin>
         <Insets left="7.0"/>
      </VBox.margin>
   </Button>
   <Button id="rankingMovie" fx:id="id_rankingMovie" defaultButton="true" mnemonicParsing="false"
           onAction="#rankingMovie" opacity="0.4" prefHeight="70.0" prefWidth="245.0" text="RANKING FILMÓW">
      <VBox.margin>
         <Insets left="7.0"/>
      </VBox.margin>
   </Button>
</VBox>
